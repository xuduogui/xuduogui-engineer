{
  "name": "@xuduogui/api-extractor-demo",
  "version": "1.0.0",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "mainEntryPointFilePath": "/<projectFolder>lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "extractor": "api-extractor run -c ./config/api-extractor.json -l -v --diagnostics",
